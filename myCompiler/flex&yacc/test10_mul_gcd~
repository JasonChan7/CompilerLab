begin
  def multiply();
  def gcd(a, b);
end
const m:=7,n:=85;
var x,y,z,q,r;
def multiply();
  begin
    var a,b;
    a:=x;b:=y;z:=0;
    while (b>0) do
    begin 
      if ODD b then 
        z:=z+a;
        a:=2*a;
        b:=b/2;
      end;
    end;
    return z;   
  end

def gcd(a, b);
  begin
    var ans;
    if b==0 then ans:=a;
    else ans:=call gcd(b, a%b); end;
    return ans;
  end

x:=m;
y:=n;
write call multiply();
x:=84;
y:=36;
write call gcd(x, y);
